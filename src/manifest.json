{
  "manifest_version": 2,
  "name": "Wind forecast | Ventibou",
  "homepage_url": "https://github.com/celian-rib/wind-browser-extension",
  "description": "Display wind forecast in your City!",
  "default_locale": "en",
  "permissions": [
    "storage",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "48": "icons/icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Wind forecast | Ventibou",
    "default_icon": {
      "38": "icons/icon.png"
    }
  }
}
